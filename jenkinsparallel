pipeline {
   agent none

   stages {
       stage('stage-1') {
         agent{label 'master'}
          steps {
              sh 'touch master'
              sh 'mkdir master-1'
         }
       }
       stage('stage-2' {
         agent {label 'java-node'}
           steps {
              sh 'touch java-node'
              sh 'mkdir java-node'
        }
      }
   }
}
