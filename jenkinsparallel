pipeline {
    agent none

    stages {
        stage('stage-1') {
            agent {
                label 'master'
            }
            steps {
                sh 'touch king'
                sh 'mkdir king1'
            }
        }
        stage('stage-2') {
            agent {
                label 'java-node'
            }
            steps {
                sh 'touch java-node'
                sh 'mkdir java-node'
            }
        }
    }
}
