
pipeline {
   agent {label 'java-node'}
   stages {
       stage('paralleljobs') {
               parallel{
               stage('stage-1A'){
                steps{
                    sh 'sleep 120'
               }
           }
        stage('stage-1B'){
                steps{
                    sh 'sleep 120'
                }
             }
          }
        }
       stage('stage-2' {
                   steps {
                           echo 'sh mkdir master-1'
                   }
              }
        }    
}
