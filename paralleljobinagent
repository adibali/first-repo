
pipeline {
   agent {label 'java-node'}
   stages {
       stage('paralleljobs') {
               parallel{
               stage('stage-1A'){
                  agent{label 'master'}
                     steps{
                    sh 'sleep 10'
               }
           }
        stage('stage-1B'){
           agent{label 'master'}
                steps{
                    sh 'sleep 10'
                }
             }
          }
        }
       stage('Deploy') {
             agent {label 'master'}
                   steps {
                           echo 'sh mkdir master-1'
                   }
              }
        }    
}
