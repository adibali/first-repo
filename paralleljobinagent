
pipeline {
   agent none
   stages {
       stage('paralleljobs') {
          agent{label 'master'}
          parallel{
          stage('stage-1A'){
                steps{
                    sh 'sleep 120'
               }
           }
        stage('stage-1B'){
                steps{
                    sh 'sleep 120'
                }
             }
          }
        }
       stage('stage-2' {
         agent {label 'java-node'}
           steps {
              sh 'sleep 60'
              echo 'sh touch master'
              echo 'sh mkdir master-1'
        }
      }
   }
}
